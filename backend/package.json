{
  "name": "drive-rwanda-prep-backend",
  "version": "1.0.0",
  "description": "Backend API for Drive Rwanda - Prep & Pass",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "seed": "node seed.js",
    "seed:force": "node seed.js --force",
    "seed:courses": "node seed.js --courses-only",
    "seed:course-contents": "node seed-course-contents.js",
    "seed:course-contents:all": "node seed-course-contents.js --all",
    "migrate": "NODE_ENV=production npx sequelize-cli db:migrate",
    "migrate:undo": "NODE_ENV=production npx sequelize-cli db:migrate:undo",
    "migrate:status": "NODE_ENV=production npx sequelize-cli db:migrate:status"
  },
  "keywords": [
    "drive-rwanda-prep",
    "exam",
    "api",
    "nodejs",
    "express"
  ],
  "author": "Code Trail",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "node-cron": "^4.2.1",
    "sequelize": "^6.35.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  }
}
